<?xml version="1.0"?>
<configuration>
	<appSettings>
		<logutil>
			<IsSwitchedOn>true</IsSwitchedOn>
			<IsTimeStampLogged>true</IsTimeStampLogged>
			<IsCategoryLogged>false</IsCategoryLogged>
			<IsReturnLogged>false</IsReturnLogged>
			<IsClassNameLogged>false</IsClassNameLogged>
			<IsMethodNameLogged>false</IsMethodNameLogged>
			<IsSeverityLogged>false</IsSeverityLogged>
			<IsSameTextCounted>false</IsSameTextCounted>
			<TextWidth>132</TextWidth>
			<DirName>"/users/peterwerk/projects/log"</DirName>
			<FileName>LogUtil</FileName>
			<HasFileNameTimestamp>true</HasFileNameTimestamp>
			<ThresholdMillis>1</ThresholdMillis>
			<FilterCategory>All</FilterCategory>
		</logutil>
		<txttohtml>
			<prefix>T2H_</prefix>
			<baseFolderPath>"/Users/peterwerk/GitHub/ToolKitConsole/GeneralUtilities/Resources"</baseFolderPath>
			<resourceFolderName>TxtToHtml</resourceFolderName>
			<inputFileName>input.txt</inputFileName>
			<indexType>Name</indexType>
			<indexTitle>Index</indexTitle>
			<outputRootFolderName>output</outputRootFolderName>
			<outputSubFolderName>files</outputSubFolderName>
			<backgroundImage>background.gif</backgroundImage>
			<styleSheet>style.css</styleSheet>
		</txttohtml>
		<csvtoxml>
			<baseFolderPath>"/Users/peterwerk/GitHub/ToolKitConsole/GeneralUtilities/Resources/CsvToXml"</baseFolderPath>
			<inputFileName>input.csv</inputFileName>
			<outputRootFolderName>output</outputRootFolderName>
			<splitChar>";"</splitChar>
		</csvtoxml>
		<xmldbmanager>
			<baseFolderPath>"/Users/peterwerk/GitHub/ToolKitConsole/GeneralUtilities/Resources/XmlDBManager</baseFolderPath>
			<dBFileName>DB.xml</dBFileName>
		</xmldbmanager>
		<outputwrapper>
			<hasConsoleOutput>true</hasConsoleOutput>
			<hasLogUtilOutput>true</hasLogUtilOutput>
			<hasXmlOutput>true</hasXmlOutput>
			<xmlDirName>"/users/peterwerk/projects/log"</xmlDirName>
			<xmlFileName>XmlLog</xmlFileName>
		</outputwrapper>
	</appSettings>
</configuration>